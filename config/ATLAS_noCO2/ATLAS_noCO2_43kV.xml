<Detector>
    <GapWidth>0.1</GapWidth>
    <Steps>500</Steps>
    <Anode>
			<width>0.12</width>
			<permittivity>5</permittivity>
    </Anode>
    <Cathode>
			<width>0.12</width>
			<permittivity>5</permittivity>
    </Cathode>
    <ElectricField>43000</ElectricField>
    <Threshold>0.1</Threshold>
    <EbarTableCalculationSteps>140</EbarTableCalculationSteps>
</Detector>

<Gas>
	<gas>
		<name>c2h2f4</name>
		<percentage>94.7</percentage>
	</gas>
	<gas>
		<name>sf6</name>
		<percentage>0.3</percentage>
	</gas>
	<gas>
		<name>isobutane</name>
		<percentage>5</percentage>
	</gas>
	<temperature>293.15</temperature>
	<pressure>760.</pressure>
</Gas>

<Simulation>
	<NoAvalanche>0</NoAvalanche>
	<OnlyMultiplication>0</OnlyMultiplication>
	
	<Particle>
		<name>muon</name>
		<momentum>5.e9</momentum>
		<x0>0.</x0>
		<theta>0</theta>
	</Particle>
	
	<Events>100</Events>
	<Threads>12</Threads>
	
	<OutFile>ATLAS_noCO2_43kV.dat</OutFile>
	
	<Generator>SFMT</Generator>
	<GlobalSeed>-1</GlobalSeed>
	<GarfieldSeed>-1</GarfieldSeed>
	
	
<!--	<SingleCluster>
		<n0>1</n0>
		<x0>-1</x0>
	</SingleCluster> -->
	
	<Verbose>1</Verbose>
	<VerbosityLevel>1</VerbosityLevel>
	<Snapshots>0</Snapshots>
	<DebugOutput>0</DebugOutput>
</Simulation>
