<Detector>
    <GapWidth>0.1</GapWidth>
    <Steps>500</Steps>
    <Anode>
			<width>0.15</width>
			<permittivity>7</permittivity>
    </Anode>
    <Cathode>
			<width>0.15</width>
			<permittivity>7</permittivity>
    </Cathode>
    <ElectricField>56000</ElectricField>
    <Threshold>0.63</Threshold>
    <EbarTableCalculationSteps>140</EbarTableCalculationSteps>
</Detector>

<Gas>
	<gas>
		<name>SF6</name>
		<percentage>100</percentage>
	</gas>
	<temperature>293.15</temperature>
	<pressure>760.</pressure>
</Gas>

<Simulation>
	<NoAvalanche>1</NoAvalanche>
	<OnlyMultiplication>0</OnlyMultiplication>
	
	<Particle>
		<name>muon</name>
		<momentum>5.e9</momentum>
		<x0>0.</x0>
		<theta>0</theta>
	</Particle>
	
	<Events>1</Events>
	<Threads>1</Threads>
	
	<OutFile>SF6.dat</OutFile>
	
	<Generator>SFMT</Generator>
	<GlobalSeed>-1</GlobalSeed>
	<GarfieldSeed>58965475</GarfieldSeed>
	
	
<!--	<SingleCluster>
		<n0>1</n0>
		<x0>-1</x0>
	</SingleCluster> -->
	
	<Verbose>1</Verbose>
	<VerbosityLevel>1</VerbosityLevel>
	<Snapshots>0</Snapshots>
	<DebugOutput>1</DebugOutput>
</Simulation>
